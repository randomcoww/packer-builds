{
  "variables": {
    "user": "randomcoww",
    "uid": "10000",
    "password": "password",
    "hostname": "docker01",
    "chef_environment": "testing",
    "encrypted_data_bag_secret_path": "/etc/chef/encrypted_data_bag_secret",
    "server_url": "https://api.opscode.com/organizations/madcoww",
    "validation_key_path": "/etc/chef/madcoww-validator.pem",
    "validation_client_name": "madcoww-validator",
    "set_fqdn": "*.dyn.local",
    "timezone": "Etc/UTC",
    "static_ip": "169.254.62.242"
  },

  "builders": [
    {
      "name": "{{user `hostname`}}",

      "type": "qemu",
      "format": "raw",
      "accelerator": "kvm",

      "iso_url": "https://download.fedoraproject.org/pub/fedora/linux/releases/23/Server/x86_64/iso/Fedora-Server-netinst-x86_64-23.iso",
      "iso_checksum": "32e0a15a1c71d0e2fd36a0af5b67a3b3af82976d2dfca0aefcb90d42f2ae6844",
      "iso_checksum_type": "sha256",

      "http_directory": "http",

      "ssh_username": "root",
      "ssh_key_path": "files/packer.key",
      "ssh_wait_timeout": "600s",
      "shutdown_command": "shutdown -P now",

      "boot_wait": "2s",
      "qemuargs": [
        [ "-display", "none" ]
      ],

      "boot_command": [
        "/install/vmlinuz ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks-fedora23.cfg ",
      ]
    }
  ]
}
